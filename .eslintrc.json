{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module"
  },
  "plugins": ["prettier"],
  "extends": ["plugin:prettier/recommended", "prettier"],
  "rules": {
    "prettier/prettier": ["warn", { "singleQuote": true }],
    "no-unused-vars": ["warn"],
    "no-undef": ["error"]
  }
}
